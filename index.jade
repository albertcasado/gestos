html
  head
    title Gestos. Multi-touch gestures iconic font
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    meta(author="https://github.com/albertcasado/gestos")
    meta(name="description", content="Gestos Multitouch gestures iconic font")
    meta(name="viewport", content="width=device-width,initial-scale=1.0, maximum-scale=1")
    link(rel="stylesheet", href="css/gestos.css")
    // collapse for presentation
    script(src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js")
    script(src="js/jquery.collapse.js")
    script(src="js/jquery.collapse_storage.js")
    script(src="js/jquery.collapse_cookie_storage.js")

    // styles just for presentation here
    style.
     *{font-family: "Hevetica Neue", Helvetica, Arial, sans-serif; font-weight:300;}
     strong {font-weight:600;}
     small {color:#7f7f7f;}
     body {width:978px; margin:0 auto;}
     h2 a{border-bottom:1px solid #999;text-decoration:none; color:#333; display:block;}
     h2 a:hover {color:#c08;}
     h2 a:before {content:"+";color:#c08; font-weight:600; padding:5px;}
     h2.open a:before {content:"-";padding-right:11px;}
     dl {-webkit-column-count:3;-moz-column-count:3;-ms-column-count:3;column-count:3; text-align:center; margin-bottom:5em;}
     dl.no-cols {-webkit-column-count:1 !important;-moz-column-count:1 !important;-ms-column-count:1 !important;column-count:1 !important;}
     dl.two-cols {-webkit-column-count:2 !important;-moz-column-count:2 !important;-ms-column-count:2 !important;column-count:2 !important;}
     dd {margin: 1em 0 2em; color:#7f7f7f;}
     dt {-webkit-break-after: avoid;-moz-break-after: avoid;break-after: avoid;}
     i{font-size:48px; padding:5px}
     dt:hover + dd {color:#c08;}
     footer {border-top:1px solid #999}

  body
